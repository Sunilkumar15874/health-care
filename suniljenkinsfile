  agent any
     tools {
       maven 'M2_HOME'
           }
     
  stages {
    stage('Git Checkout') {
      steps {
        echo 'This stage is to clone the repo from github'
        git branch: 'main', url: 'https://github.com/Sunilkumar15874/health-care.git'
                        }
            }
 
